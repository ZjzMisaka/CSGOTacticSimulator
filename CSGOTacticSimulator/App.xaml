<Application x:Class="CSGOTacticSimulator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CSGOTacticSimulator"
             StartupUri="Screens/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="i18n\StringResource.zh-CN.xaml" />
                <ResourceDictionary Source="i18n\StringResource.ja-JP.xaml" />
                <ResourceDictionary Source="i18n\StringResource.en-US.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
